<?xml version="1.0"?>
<xs:schema targetNamespace="eml://ecoinformatics.org/entity-2.0.0beta9"
    xmlns="eml://ecoinformatics.org/entity-2.0.0beta9" 
    xmlns:att="eml://ecoinformatics.org/attribute-2.0.0beta9"
    xmlns:prot="eml://ecoinformatics.org/protocol-2.0.0beta9" 
    xmlns:phys="eml://ecoinformatics.org/physical-2.0.0beta9"
    xmlns:cov="eml://ecoinformatics.org/coverage-2.0.0beta9" 
    xmlns:doc="eml://ecoinformatics.org/documentation-2.0.0beta9"
    xmlns:res="eml://ecoinformatics.org/resource-2.0.0beta9" 
    xmlns:con="eml://ecoinformatics.org/constraint-2.0.0beta9"
    xmlns:xs="http://www.w3.org/2001/XMLSchema">
