<xs:schema targetNamespace="eml:spatialRaster-2.0.0beta8" xmlns="eml:spatialRaster-2.0.0beta8" xmlns:phys="eml:physical-2.0.0beta8" xmlns:att="eml:attribute-2.0.0beta8" xmlns:prot="eml:protocol-2.0.0beta8" xmlns:ent="eml:entity-2.0.0beta8" xmlns:cov="eml:coverage-2.0.0beta8" xmlns:doc="eml:documentation-2.0.0beta8" xmlns:xs="http://www.w3.org/2001/XMLSchema">
