  schemaLocation="eml-spatialReference.xsd" />
  <xs:import namespace="eml://ecoinformatics.org/entity-2.0.0rc2"
  schemaLocation="eml-entity.xsd" />
  <xs:import namespace="eml://ecoinformatics.org/resource-2.0.0rc2"
  schemaLocation="eml-resource.xsd" />
