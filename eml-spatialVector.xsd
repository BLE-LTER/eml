    targetNamespace="eml://ecoinformatics.org/spatialVector-2.0.0rc2"
    xmlns="eml://ecoinformatics.org/spatialVector-2.0.0rc2" 
    xmlns:ent="eml://ecoinformatics.org/entity-2.0.0rc2"
    xmlns:spref="eml://ecoinformatics.org/spatialReference-2.0.0rc2"
    xmlns:att="eml://ecoinformatics.org/attribute-2.0.0rc2"
    xmlns:con="eml://ecoinformatics.org/constraint-2.0.0rc2"
    xmlns:doc="eml://ecoinformatics.org/documentation-2.0.0rc2" 
    xmlns:res="eml://ecoinformatics.org/resource-2.0.0rc2"
    xmlns:xs="http://www.w3.org/2001/XMLSchema">
