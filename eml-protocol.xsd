    <xs:appinfo>
      <moduleName>
        eml-quality
      </moduleName>
      <moduleDescription>
        The EML Quality Module is used to identify the proccesses that have been
	used to define / improve the quality of a data file, also used to
	identify potential problems with the data file.
      </moduleDescription>
    </xs:appinfo>
  </xs:annotation>

<xs:element name="eml-quality">
<xs:complexType>
<xs:sequence>
	<xs:element name = "identifier" type = "xs:string"
                    minOccurs="1" maxOccurs="unbounded">
          <xs:complexType>
            <xs:attribute name="system" use="optional" type="xs:string">         
            </xs:attribute>
          </xs:complexType>
        </xs:element>
    
	<xs:element name="step" 
		minOccurs="1" maxOccurs="1">
		
	    <xs:annotation>
            <xs:appinfo>
              <tooltip> </tooltip>
              <summary> </summary>
              <description>Step defines the quality control / assurance procedures 
	      that were applied to the corresponding data file.  For example, 
	      a shell-script that 'greps' for an alpha-character within a 
	      datafile that is supposed to contain only numeric characters
	      </description>
              <example> </example>
              <lineage> </lineage>
            </xs:appinfo>
          </xs:annotation>
		
	 <xs:complexType>
    	  <xs:sequence>
   
      		<xs:element name="purpose" type="paragraph"
			minOccurs="1" maxOccurs="unbounded">
			
	    <xs:annotation>
            <xs:appinfo>
              <tooltip> </tooltip>
              <summary> </summary>
              <description> This refers to the motivation for designing or 
	      implementing the quality control / assurance process to the data 
	      file. 
	      </description>
              <example> </example>
              <lineage> </lineage>
            </xs:appinfo>
          </xs:annotation>
		</xs:element>
      		<xs:element name="action" type="paragraph"
			minOccurs="1" maxOccurs="unbounded">
	    <xs:annotation>
            <xs:appinfo>
              <tooltip> </tooltip>
              <summary> </summary>
              <description> The action element describes the procedure taken  
	      in the quality control / assurance process performed on the data 
	      file. 
	      </description>
              <example> </example>
              <lineage> </lineage>
            </xs:appinfo>
          </xs:annotation>
	</xs:element>
	<xs:element name="outcome" type="paragraph"
			minOccurs="1" maxOccurs="unbounded">
	    <xs:annotation>
            <xs:appinfo>
              <tooltip> </tooltip>
              <summary> </summary>
              <description> The outcome expected after the quality control / 
	      assurance procedure is applied on the data file. 
	      </description>
              <example> </example>
              <lineage> </lineage>
            </xs:appinfo>
          </xs:annotation>
	</xs:element>
	<xs:element name="appliedAttribute" type="paragraph"
			minOccurs="0" maxOccurs="unbounded">
	    <xs:annotation>
            <xs:appinfo>
              <tooltip> </tooltip>
              <summary> </summary>
              <description> An attribute of the data file which this quality
	      control / assurance process was applied to
	      </description>
              <example> </example>
              <lineage> </lineage>
            </xs:appinfo>
          </xs:annotation>	
	</xs:element>
      	<xs:element name="contact" type="rp:responsibleParty"
			minOccurs="0" maxOccurs="unbounded">
	<xs:annotation>
            <xs:appinfo>
              <tooltip> </tooltip>
              <summary> </summary>
              <description> The person responsible of performing / difining this
	      process on the attribute in the data file
	      </description>
              <example> </example>
              <lineage> </lineage>
            </xs:appinfo>
         </xs:annotation>
	</xs:element>      
    </xs:sequence>
   </xs:complexType>
  </xs:element>
  <xs:element name="knownProblems" type="paragraph"
                    minOccurs="0" maxOccurs="unbounded">
    <xs:annotation>
      <xs:appinfo>
         <tooltip> </tooltip>
         <summary> </summary>
         <description> A description of the problems that might be associated 
	 with the implementation of this quality control / assurance procedure
	 </description>
         <example> </example>
         <lineage> </lineage>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  <xs:element name="qualityCode" type=""
                    minOccurs="0" maxOccurs="unbounded">
   <xs:attribute name="codeSystem" use="optional" type="xs:string">
	    <xs:annotation>
        	<xs:appinfo>
              	  <tooltip> </tooltip>
              	  <summary> </summary>
              	  <description> Give the name of the coding system utilized for 
		  assigning quality control / assurance codes to the data file
	      </description>
              <example> </example>
              <lineage> </lineage>
            </xs:appinfo>
          </xs:annotation>
   </xs:attribute>
     <xs:complexType>
       <xs:sequence>
 	<xs:element name="description" type="paragraph"
			minOccurs="0" maxOccurs="1">
	    <xs:annotation>
        	<xs:appinfo>
              	  <tooltip> </tooltip>
              	  <summary> </summary>
              	  <description> The description of the coding value utilized 
		  for assigning quality control / assurance to the data 
		  file
	      </description>
              <example> </example>
              <lineage> </lineage>
            </xs:appinfo>
          </xs:annotation>
		
	</xs:element>
      		<xs:element name="code" type="paragraph"
			minOccurs="0" maxOccurs="unbounded">
		
		   <xs:annotation>
        	<xs:appinfo>
              	  <tooltip> </tooltip>
              	  <summary> </summary>
          	  <description> This is a code which refers to the quality and 
		  is specified in the code system that is used.  For example a 
		  code system may have the value of NASA and the specific code
		  designated by this attribute may have a value = "1". </description>
              <example> </example>
              <lineage> </lineage>
            </xs:appinfo>
          </xs:annotation>
		
		
		</xs:element>
		
		
    </xs:sequence>
   </xs:complexType>
  </xs:element>
      
       
</xs:sequence>
</xs:complexType>
</xs:element>
      
</xs:schema>
