  <xs:import namespace="eml://ecoinformatics.org/protocol-2.0.0beta9"
  schemaLocation="eml-protocol.xsd" />
  <xs:import namespace="eml://ecoinformatics.org/physical-2.0.0beta9"
  schemaLocation="eml-physical.xsd" />
  <xs:import namespace="eml://ecoinformatics.org/coverage-2.0.0beta9"
  schemaLocation="eml-coverage.xsd" />
  <xs:import namespace="eml://ecoinformatics.org/resource-2.0.0beta9"
  schemaLocation="eml-resource.xsd" />
  <xs:import namespace="eml://ecoinformatics.org/constraint-2.0.0beta9"
  schemaLocation="eml-constraint.xsd" />
