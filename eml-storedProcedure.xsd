xmlns:ent="eml://ecoinformatics.org/entity-2.0.0beta9" xmlns:res="eml://ecoinformatics.org/resource-2.0.0beta9"
xmlns:con="eml://ecoinformatics.org/constraint-2.0.0beta9"
xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="eml://ecoinformatics.org/entity-2.0.0beta9"
  schemaLocation="eml-entity.xsd" />
  <xs:import namespace="eml://ecoinformatics.org/documentation-2.0.0beta9"
