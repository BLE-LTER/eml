<?xml version="1.0"?>
<xs:schema targetNamespace="eml://ecoinformatics.org/attribute-2.0.0beta9" xmlns="eml://ecoinformatics.org/attribute-2.0.0beta9" xmlns:md="eml://ecoinformatics.org/methods-2.0.0beta9" xmlns:cov="eml://ecoinformatics.org/coverage-2.0.0beta9" xmlns:lit="eml://ecoinformatics.org/literature-2.0.0beta9" xmlns:doc="eml://ecoinformatics.org/documentation-2.0.0beta9" xmlns:res="eml://ecoinformatics.org/resource-2.0.0beta9" xmlns:xs="http://www.w3.org/2001/XMLSchema">
