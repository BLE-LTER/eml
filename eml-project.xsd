              <xs:element name="frequency" type="xs:string"
                          minOccurs="0" maxOccurs="1"/>
              <xs:element name="paragraph" type="xs:string"
                          minOccurs="0" maxOccurs="unbounded"/>
              <xs:element name="citation" type="cit:LitCitation"
                          minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        
        <xs:element name="paragraph" type="xs:string"
                    minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="citation" type="cit:LitCitation"
                    minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
        
  <xs:element name="methodsDescription">
    <xs:annotation>
      <xs:appinfo>
        <tooltip>
          Research method 
        </tooltip>
        <summary>
          Research method descriptor
        </summary>
        <description>
          Description of research method of the project:
          the method itself, instruments used, taxonomy, permit, 
          legal information, 
          descriptive paragraph of text or citation from a source.
        </description>
        <example>
        </example>
        <lineage>
          The methodsDecription element was introduced into EML 1.4.
        </lineage>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="method" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:appinfo>
              <tooltip>
                Research method 
              </tooltip>
              <summary>
                Research method descriptor
              </summary>
              <description>
                Description or reference to field/laboratory method:
                descriptive paragraph of text or citation from a source.
              </description>
              <example>
              </example>
              <lineage>
                The method element was introduced into EML 1.4.
              </lineage>
            </xs:appinfo>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="paragraph" type="xs:string"
                          minOccurs="0" maxOccurs="unbounded"/>
              <xs:element name="citation" type="cit:LitCitation"
                          minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>

        <xs:element name="instrument" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:appinfo>
              <tooltip>
                Instrument used 
              </tooltip>
              <summary>
                Instrument used
              </summary>
              <description>
                Instruments used:
                descriptive paragraph of text or citation from a source.
              </description>
              <example>
              </example>
              <lineage>
                The instrument element was introduced into EML 1.4.
              </lineage>
            </xs:appinfo>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="paragraph" type="xs:string"
                          minOccurs="0" maxOccurs="unbounded"/>
              <xs:element name="citation" type="cit:LitCitation"
                          minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        
        <xs:element name="taxonomy" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:appinfo>
              <tooltip>
                Taxonomy
              </tooltip>
              <summary>
                Taxonomy information
              </summary>
