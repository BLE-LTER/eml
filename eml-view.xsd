<xs:schema targetNamespace="eml:view-2.0.0beta9" xmlns="eml:view-2.0.0beta9"
xmlns:cov="eml:coverage-2.0.0beta9" xmlns:prot="eml:protocol-2.0.0beta9"
xmlns:phys="eml:physical-2.0.0beta9" xmlns:att="eml:attribute-2.0.0beta9"
xmlns:ent="eml:entity-2.0.0beta9" xmlns:res="eml:resource-2.0.0beta9"
xmlns:xs="http://www.w3.org/2001/XMLSchema"
xmlns:con="eml:constraint-2.0.0beta9" xmlns:doc="eml:documentation-2.0.0beta9">
  <xs:import namespace="eml:entity-2.0.0beta9"
  schemaLocation="eml-entity.xsd" />
  <xs:import namespace="eml:documentation-2.0.0beta9"
  schemaLocation="eml-documentation.xsd" />
  <xs:import namespace="eml:attribute-2.0.0beta9"
  schemaLocation="eml-attribute.xsd" />
  <xs:import namespace="eml:protocol-2.0.0beta9"
  schemaLocation="eml-protocol.xsd" />
  <xs:import namespace="eml:physical-2.0.0beta9"
  schemaLocation="eml-physical.xsd" />
  <xs:import namespace="eml:coverage-2.0.0beta9"
  schemaLocation="eml-coverage.xsd" />
  <xs:import namespace="eml:resource-2.0.0beta9"
  schemaLocation="eml-resource.xsd" />
  <xs:import namespace="eml:constraint-2.0.0beta9"
  schemaLocation="eml-constraint.xsd" />
  <xs:annotation>
    <xs:documentation>
