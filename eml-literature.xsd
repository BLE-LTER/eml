          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="publisher" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <doc:tooltip>Publishing company</doc:tooltip>
            <doc:summary/>
            <doc:description/>
            <doc:example/>
            <doc:lineage>This field was introduced into EML 2.0 beta 1, and was 
                  derived from MARC/various software packages handling literature</doc:lineage>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="pageRange" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:appinfo>
            <doc:tooltip>Beginning and ending page of chapter</doc:tooltip>
            <doc:summary/>
            <doc:description/>
            <doc:example/>
            <doc:lineage>This field was introduced into EML 2.0 beta 1, and was 
                  derived from MARC/various software packages handling literature</doc:lineage>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="edition" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:appinfo>
            <doc:tooltip>Edition of book</doc:tooltip>
            <doc:summary/>
            <doc:description/>
            <doc:example/>
            <doc:lineage>This field was introduced into EML 2.0 beta 1, and was 
                  derived from MARC/various software packages handling literature</doc:lineage>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="volume" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:appinfo>
            <doc:tooltip>Volume of book if part of a series</doc:tooltip>
            <doc:summary/>
            <doc:description/>
            <doc:example/>
            <doc:lineage>This field was introduced into EML 2.0 beta 1, and was 
                  derived from MARC/various software packages handling literature</doc:lineage>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Manuscript">
    <xs:sequence>
      <xs:element name="pubInfo" type="rp:ResponsibleParty">
        <xs:annotation>
          <xs:appinfo>
            <doc:tooltip>address where manuscript may be requested</doc:tooltip>
            <doc:summary/>
            <doc:description/>
            <doc:example/>
            <doc:lineage>This field was introduced into EML 2.0 beta 1, and was 
                  derived from MARC/various software packages handling literature</doc:lineage>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="Location" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Report">
    <xs:sequence>
      <xs:element name="institution" type="rp:ResponsibleParty">
        <xs:annotation>
          <xs:appinfo>
            <doc:tooltip>Responsible institution where report may be requested</doc:tooltip>
            <doc:summary/>
            <doc:description/>
            <doc:example/>
            <doc:lineage>This field was introduced into EML 2.0 beta 1, and was 
                  derived from MARC/various software packages handling literature</doc:lineage>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="reportNumber" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <doc:tooltip>ID number of report</doc:tooltip>
            <doc:summary/>
            <doc:description/>
            <doc:example/>
            <doc:lineage>This field was introduced into EML 2.0 beta 1, and was 
                  derived from MARC/various software packages handling literature</doc:lineage>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Thesis">
    <xs:sequence>
      <xs:element name="degree" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <doc:tooltip>Name of degree</doc:tooltip>
            <doc:summary/>
            <doc:description/>
