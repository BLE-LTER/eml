          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="volume" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:appinfo>
            <doc:tooltip>Volume if book is part of series</doc:tooltip>
            <doc:summary/>
            <doc:description/>
            <doc:example/>
            <doc:lineage>This field was introduced into EML 2.0 beta 1, and was 
                  derived from MARC/various software packages handling literature</doc:lineage>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Chapter">
    <xs:sequence>
      <xs:element name="chapterNumber" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:appinfo>
            <doc:tooltip>Chapter number of interest within book</doc:tooltip>
            <doc:summary/>
            <doc:description/>
            <doc:example/>
            <doc:lineage>This field was introduced into EML 2.0 beta 1, and was 
                  derived from MARC/various software packages handling literature</doc:lineage>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="bookEditor" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <doc:tooltip>Name of book editor</doc:tooltip>
            <doc:summary/>
            <doc:description>The editor may be a person
                  or organization or a role within an organization.</doc:description>
            <doc:example/>
            <doc:lineage>This field was introduced into EML 2.0 beta 1, and was 
                  derived from MARC/various software packages handling literature</doc:lineage>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="bookTitle" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <doc:tooltip>Title of book</doc:tooltip>
            <doc:summary/>
