      <xs:element name="degreeInstitution" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <doc:tooltip>Degree awarding institution</doc:tooltip>
            <doc:summary/>
            <doc:description/>
            <doc:example/>
            <doc:lineage>This field was introduced into EML 2.0 beta 1, and was 
                  derived from MARC/various software packages handling literature</doc:lineage>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:schema>
