    <xs:appinfo>
      <doc:moduleDocs>
        <doc:moduleName>eml-spatialImage</doc:moduleName>
        <doc:moduleDescription>
          This module describes a spatially referenced image.  Attributes like
          ground spacing, image suitability sensor parameters, sensor bands and
          imaging condition codes can be stored in this module.
        </doc:moduleDescription>
        <doc:recommendedUsage>
          all spatial datasets that contains spatially referenced images
        </doc:recommendedUsage>
        <doc:standAlone>
          yes
        </doc:standAlone>
        <doc:relationship>
          <doc:moduleName>
            eml-dataset
          </doc:moduleName>
          <doc:relationshipType>
            has spatial image information for
          </doc:relationshipType>
        </doc:relationship>
      </doc:moduleDocs>
    </xs:appinfo>
  </xs:annotation>
  
  <xs:complexType name="spatialImageContainer">
    <xs:choice>
      <xs:element ref="spatialImage"/>
      <xs:element name="references" type="xs:string"/>
    </xs:choice>
  </xs:complexType>
  
  <xs:element name="spatialImage" type="SpatialImage">
    <xs:annotation>
      <xs:appinfo>
        <doc:tooltip>
          This element describes a spatially referenced image.
        </doc:tooltip>
        <doc:summary>
          This element describes a spatially referenced image.
        </doc:summary>
        <doc:description>
          This element describes a spatially referenced image. This could be 
          a GIS layer or any other spatially referenced visual
        </doc:description>
        <doc:example>
           arcview .arc or .arg file.
        </doc:example>
        <doc:lineage>
          The spatial entity elements were introduced into EML 2.0 beta 7
        </doc:lineage>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
